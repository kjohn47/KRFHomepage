{
  "AppConfiguration": {
    "ApiName": "KRFHomepage",
    "TokenIdentifier": "AccessToken",
    "TokenKey": "APICOMMONSIGNEDKEY"
  },
  "Logging": {
    "IncludeScopes": {},
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KRFDatabases": {
    "Databases": {
      "HomepageDBContext": {
        "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=HomeDB;AttachDbFilename=DBFiles\\HomeDB.mdf;Trusted_Connection=True;MultipleActiveResultSets=true",
        "UseLocalDB": true
      }
    },
    "EnableAutomaticMigration": true,
    "MigrationAssembly": "KRFHomepage.Infrastructure"
  },
  "KestrelConfiguration": {
    "HttpPort": 4747,
    "HttpsPort": 14747
  },
  "RequestContext": {
    "EnableRead": false,
    "MemBufferOnly": true,
    "BufferSize": 0
  },
  "LogsOnPrd": false,
  "KRFMemoryCacheSettings": {
    "CacheCleanupInterval": {
      "Hours": 0,
      "Minutes": 30,
      "Seconds": 0
    },
    "CachedKeySettings": {
      "TRANSLATION_CODES": {
        "RemoveHours": 0,
        "RemoveMinutes": 60,
        "RemoveSeconds": 0
      },
      "TRANSLATION_SETTINGS": {
        "RemoveHours": 0,
        "RemoveMinutes": 30,
        "RemoveSeconds": 0
      }
    }
  }
}
